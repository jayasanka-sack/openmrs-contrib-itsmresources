datadog_tags_extra:
  - "service:ocl"

docker_deployment:
  - oclclient-stg
  - oclclient-demo

docker_deployable_images:
  - repository: openmrs/oclclient-stg
    compose: oclclient-stg
  - repository: openmrs/oclclient-demo
    compose: oclclient-demo  


letsencrypt_cert_domains:
   - nakuru.openmrs.org
   - openmrs.demo.openconceptlab.org
   - openmrs.staging.openconceptlab.org

nginx_keepalive_timeout: "600"
nginx_keepalive_requests: "600"

nginx_vhosts:
  - listen: "443 ssl"
    server_name: "nakuru.openmrs.org"
    extra_parameters: |
      access_log /var/log/nginx/deployer_access.log;
      error_log /var/log/nginx/deployer_error.log;
      ssl_certificate /etc/letsencrypt/live/nakuru.openmrs.org/fullchain.pem;
      ssl_certificate_key /etc/letsencrypt/live/nakuru.openmrs.org/privkey.pem;
      location ^~ /.well-known/acme-challenge/ {
        root /usr/share/nginx/html;
      }
      location / {
        proxy_pass http://127.0.0.1:3000/;
      }
  - listen: "80"
    server_name: "openmrs.demo.openconceptlab.org"
    filename: "openmrs.demo.openconceptlab.org.80.conf"
    extra_parameters: |
      return 301 https://$host$request_uri;
  - listen: "443 ssl"
    server_name: "openmrs.demo.openconceptlab.org"
    extra_parameters: |
      access_log /var/log/nginx/oclclient_access.log;
      error_log /var/log/nginx/oclclient_error.log;
      ssl_certificate /etc/letsencrypt/live/nakuru.openmrs.org/fullchain.pem;
      ssl_certificate_key /etc/letsencrypt/live/nakuru.openmrs.org/privkey.pem;
      location ^~ /.well-known/acme-challenge/ {
        root /usr/share/nginx/html;
      }
      location / {
        proxy_pass http://127.0.0.1:8081/;
      }
  - listen: "80"
    server_name: "openmrs.staging.openconceptlab.org"
    filename: "openmrs.staging.openconceptlab.org.80.conf"
    extra_parameters: |
      return 301 https://$host$request_uri;
  - listen: "443 ssl"
    server_name: "openmrs.staging.openconceptlab.org"
    extra_parameters: |
      access_log /var/log/nginx/oclclientstg_access.log;
      error_log /var/log/nginx/oclclientstg_error.log;
      ssl_certificate /etc/letsencrypt/live/nakuru.openmrs.org/fullchain.pem;
      ssl_certificate_key /etc/letsencrypt/live/nakuru.openmrs.org/privkey.pem;
      location ^~ /.well-known/acme-challenge/ {
        root /usr/share/nginx/html;
      }
      location / {
        proxy_pass http://127.0.0.1:8085/;
      }

aws_access_key_id: '{{ vault_aws_access_key_id }}'
